(defmacro (comparator pred pivot)
  (list 'lambda (list 'it) (list pred 'it pivot)))

(define (qsort lis)
  (when lis
    (let ((pivot (car lis))
           (lis (cdr lis)))
      (append
        (qsort (filter (comparator < pivot) lis))
        (list pivot)
        (qsort (filter (comparator >= pivot) lis))))))

(print (qsort (list 5 6 2 1 4 5 2 6 3 9 1)))
